<div class="row">
  <div class="col-md-10">
    <label class="control-label"></label>
    <div class="form-box">
      <form class="form-horizontal" id="vol-select-form" name="aform" >
    <div class="messages"></div>
    {{#each volumes}}
        <div class="form-group">
            <label class="control-label col-sm-3" for="{{this.id}}">{{this.label}}<span class="required">*</span></label>
            <div class="col-sm-5">
                <select class="form-control" type="text" id="{{this.id}}" name="{{this.id}}" data-placeholder="Select share" >
                    <option></option>
                    {{#each ../shares}}
                    {{#unless (isSystem this)}}
                        <option value="{{this.name}}">{{this.name}}</option>
                    {{/unless}}
                    {{/each}}
               </select>
           </div>
           <i class="fa fa-info-circle fa-lg" title="{{this.description}}"></i>
         </div>
     {{/each}}
      </form>
    </div>
  </div>
</div>
